	"types": "dist/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"lint": "eslint src --ext ts,tsx",
		"watch": "tsc -w",
		"build": "tsc -d"
	},
	"author": "Jonny Burger",
	"contributors": [],
	"license": "MIT",
	"repository": {
		"url": "https://github.com/remotion-dev/remotion"
	},
