{
	"name": "@remotion/transitions",
	"version": "4.0.20",
	"description": "Transition presets for Remotion",
	"sideEffects": false,
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"lint": "eslint src --ext ts,tsx",
		"watch": "tsc -w",
		"build": "tsc -d"
	},
	"author": "Jonny Burger",
	"contributors": [],
	"license": "MIT",
	"repository": {
		"url": "https://github.com/remotion-dev/remotion"
	},
	"bugs": {
		"url": "https://github.com/remotion-dev/remotion/issues"
	},
	"dependencies": {
		"remotion": "workspace:*"
	},
	"devDependencies": {
		"@jonny/eslint-config": "3.0.273",
		"eslint": "8.42.0",
		"prettier": "^2.0.5",
		"prettier-plugin-organize-imports": "^3.2.2",
		"remotion": "workspace:*",
		"react": "18.0.0",
		"react-dom": "18.0.0",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.10"
	},
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"keywords": [
		"remotion",
		"transitions"
	],
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./Fade": {
			"import": "./dist/presentations/Fade.js",
			"types": "./dist/presentations/Fade.d.ts"
		},
		"./Slide": {
			"import": "./dist/presentations/Slide.js",
			"types": "./dist/presentations/Slide.d.ts"
		},
		"./Wipe": {
			"import": "./dist/presentations/Wipe.js",
			"types": "./dist/presentations/Wipe.d.ts"
		},
		"./package.json": "./package.json"
	},
	"typesVersions": {
		">=1.0": {
			"Wipe": [
				"dist/presentations/Wipe.d.ts"
			],
			"Slide": [
				"dist/presentations/Slide.d.ts"
			],
			"Fade": [
				"dist/presentations/Fade.d.ts"
			]
		}
	}
}
