		"url": "https://github.com/remotion-dev/remotion/issues"
	},
	"dependencies": {
		"remotion": "workspace:*"
	},
	"devDependencies": {
		"@jonny/eslint-config": "3.0.266",
		"eslint": "8.42.0",
		"prettier": "^2.0.5",
		"prettier-plugin-organize-imports": "^3.2.2",
		"remotion": "workspace:*",
		"react": "18.0.0",
		"react-dom": "18.0.0",
		"@types/react": "18.0.26",
		"@types/react-dom": "^18.0.5"
	},
	"peerDependencies": {
