{
	"extends": "@jonny",
	"rules": {
		"no-console": "error",
		"no-restricted-imports": [
			"error",
			{
				"patterns": [
					"@remotion/*/src/*",
					"@remotion/renderer/*/src/*",
					"@remotion/cli/*/src/*"
				]
			}
		]
	},
	"overrides": [
		{
			"files": "./src/gcpInstaller/**",
			"rules": {
				"no-console": "off"
			}
		},
		{
			"files": "./src/admin/**",
			"rules": {
				"no-console": "off"
			}
		}
	]
}
