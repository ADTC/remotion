{
  "steps": [
    {
      "name": "gcr.io/cloud-builders/docker",
      "entrypoint": "bash",
      "args": [
        "-c",
        "docker pull us-docker.pkg.dev/remotion-dev/development/render:cachebase || exit 0"
      ]
    },
    {
      "name": "gcr.io/cloud-builders/docker",
      "args": [
        "build",
        "-t",
        "us-docker.pkg.dev/remotion-dev/development/render:4.1.0-alpha6",
        "--cache-from",
        "us-docker.pkg.dev/remotion-dev/development/render:cachebase",
        "."
      ]
    }
  ],
  "images": [
    "us-docker.pkg.dev/remotion-dev/development/render:4.1.0-alpha6"
  ]
}