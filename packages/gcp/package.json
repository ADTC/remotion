{
	"name": "@remotion/gcp",
	"version": "0.0.1",
	"description": "GCP alternative to lambda rendering",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc -d && pnpm run buildContainer",
		"buildContainer": "ts-node src/admin/bundle-renderLogic.ts",
		"start": "functions-framework --target=TypescriptFunction",
		"watch": "tsc -w",
		"lint": "eslint src --ext ts,tsx"
	},
	"bin": {
		"remotion-gcp": "remotion-gcp-cli.mjs"
	},
	"dependencies": {
		"@google-cloud/functions-framework": "^3.1.3",
		"@google-cloud/storage": "^6.9.1",
		"@google-cloud/run": "^0.4.0",
		"@remotion/bundler": "3.3.59",
		"@remotion/cli": "3.3.59",
		"@remotion/renderer": "3.3.59",
		"remotion": "3.3.59"
	},
	"devDependencies": {
		"prettier-plugin-organize-imports": "^3.2.2",
		"typescript": "^4.7.0",
		"prettier": "^2.7.1",
		"@remotion/compositor-linux-x64-musl": "3.3.59",
		"ts-node": "^10.8.0",
		"@types/minimist": "1.2.2",
		"eslint": "8.25.0",
		"@jonny/eslint-config": "3.0.266"
	}
}
