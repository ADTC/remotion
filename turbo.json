{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "@remotion/example#lint": {
      "dependsOn": ["@remotion/eslint-config#build"]
    },
    "@remotion/it-tests#test": {
      "dependsOn": ["@remotion/example#build"]
    }
  },
  "npmClient": "pnpm"
}
